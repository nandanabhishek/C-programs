
#include <stdio.h>

int main()
{
    int n1,n2,n3;
    printf("Enter any 3 integers :");
    scanf("%d%d%d",&n1,&n2,&n3);
    printf("Largest integer is ");
    if (n1 > n2)
    {
        if (n1 > n3)
            printf("%d", n1);
        else
            printf("%d", n3);
    }
    else if(n2>n3)
    {
        if (n2 > n1)
            printf("%d", n2);
        else
            printf("%d", n1);
    }
    else if(n3>n1)
    {
        if (n3 > n2)
            printf("%d", n3);
        else
            printf("%d", n2);
    }

}
